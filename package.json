{
  "name": "teesa-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "nft": "pnpm --filter @teesa-monorepo/nft initialize",
    "dev": "pnpm --filter @teesa-monorepo/agent build && pnpm --filter @teesa-monorepo/web-app dev",
    "start": "pnpm --filter @teesa-monorepo/agent build && pnpm --filter @teesa-monorepo/twitter-client start && pnpm --filter @teesa-monorepo/web-app start",
    "clean": "pnpm -r clean && bash ./scripts/clean.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@types/node": "^22.13.14",
    "typescript": "~5.7.2"
  }
}